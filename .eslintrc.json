{
    "env": {
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "array-bracket-spacing": "warn",
      "indent": [
        "warn",
        2
      ],
      "linebreak-style": [
        "warn",
        "unix"
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "semi": [
        "warn",
        "always"
      ]
    }
  }